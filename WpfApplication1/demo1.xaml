<Window x:Class="WpfApplication1.demo1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="demo1" Height="300" Width="300">
    <Grid>
        <TextBox
			Height="29"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Stretch"
			VerticalAlignment="Top"
			Margin="69.5,67,69.5,0"
			x:Name="win1_textinput" />
        <Button
			Content="Submit"
			Height="36"
			Width="97"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Top"
			Margin="97,148,0,0"
			x:Name="button1"
			Click="button1_Click" />
        <Button
			Height="37"
			Width="90"
			Grid.Column="0"
			Grid.Row="0"
			HorizontalAlignment="Left"
			VerticalAlignment="Bottom"
			Margin="102,0,0,17"
			x:Name="readcsv"
			Content="Read CSV"
			Click="readcsv_Click" />
    

    </Grid>
</Window>
